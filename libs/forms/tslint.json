{
  "extends": "../../tslint.json",
  "rules": {
    "component-selector": [true, "element", "dyn", "kebab-case"],
    "directive-selector": [true, "attribute", "dyn", "camelCase"],
    "directive-class-suffix": false,
    "no-input-rename": false
